        <process id="cn.ibizlab.core.resource.service.logic.resource_resourcebefore_create" isClosed="false" isExecutable="true" name="resource_resource_resource_before_create_ruleFlow" processType="Private" tns:packageName="cn.ibizlab.core.resource.service.logic.resource_resourcebefore_create">
