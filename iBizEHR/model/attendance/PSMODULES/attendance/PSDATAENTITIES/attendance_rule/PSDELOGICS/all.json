    "sql" : "SELECT \r\n    RULE_ID,\r\n    COUNT(DISTINCT MEMBER_ID) AS total_members\r\nFROM attendance_schedule\r\nWHERE checkin_date >= CURDATE()\r\nGROUP BY RULE_ID",
