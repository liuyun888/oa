  "getPSDEFormItemUpdates" : [ {
    "codeName" : "record_duration",
    "name" : "记录时长",
    "getPSDEFIUpdateDetails" : [ {
      "name" : "business_days"
    }, {
      "name" : "overtime_hours"
    }, {
      "name" : "overtime_minutes"
    }, {
      "name" : "days"
    }, {
      "name" : "minutes"
    }, {
      "name" : "go_out_hours"
    }, {
      "name" : "leave_days"
    }, {
      "name" : "hours"
    } ],
    "scriptCode" : "let tempData = data;\r\nif (data.start_time && data.end_time) {\r\n    let start_time = new Date(data.start_time);\r\n    let end_time = new Date(data.end_time);\r\n    var apply_type = data.apply_type;\r\n    // 将秒置为零\r\n    start_time.setSeconds(0);\r\n    end_time.setSeconds(0);\r\n    let daysTemp = 0;\r\n    let hoursTemp = 0;\r\n    let minutesTemp = 0;\r\n    let timeDifference = end_time - start_time;\r\n\r\n    hoursTemp = Math.round((timeDifference / (1000 * 3600)) * 10) / 10;\r\n    daysTemp = Math.round((timeDifference / (1000 * 3600 * 24)) * 10) / 10;\r\n\r\n    if (apply_type == \"OVERTIME\") {\r\n        hoursTemp = Math.floor(timeDifference / (1000 * 3600));\r\n        minutesTemp = Math.floor((timeDifference % (1000 * 3600)) / (1000 * 60));\r\n    }\r\n    \r\n    tempData = Object.assign({}, data, {\r\n        days: daysTemp,\r\n        hours: hoursTemp,\r\n        minutes: minutesTemp,\r\n        go_out_hours: hoursTemp + \"小时\",\r\n        leave_days: daysTemp + \"天\",\r\n        business_days: daysTemp + \"天\",\r\n        overtime_minutes: minutesTemp,\r\n        overtime_hours: hoursTemp\r\n    })\r\n}\r\nreturn tempData;",
    "customCode" : true
  } ],
