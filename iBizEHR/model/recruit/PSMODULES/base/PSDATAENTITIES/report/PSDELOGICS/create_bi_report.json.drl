    no-loop

            //逻辑处理节点[开始]
            rule "begin"
            ruleflow-group "reportcreate_bi_reportbegin"
            when
            then
            end

            //逻辑处理节点[从模板初始化模型]
            rule "rawsfcode1"
            ruleflow-group "reportcreate_bi_reportrawsfcode1"
            when
            then
                        update(reportcreate_bi_reportdefault);//更新fact中变量值
                        update(reportcreate_bi_reportbireport);//更新fact中变量值
            end

            //逻辑处理节点[构造智能报表默认参数]
            rule "prepareparam1"
            ruleflow-group "reportcreate_bi_reportprepareparam1"
            when
            then
                            reportcreate_bi_reportbireport.set("validflag","1");
                            reportcreate_bi_reportbireport.set("pssysbischemeid","BIScheme");
                            reportcreate_bi_reportbireport.set("pssysbicubeid","BIScheme.applicant");
                            reportcreate_bi_reportbireport.set("pssysbireportname",reportcreate_bi_reportdefault.get("name"));
                        update(reportcreate_bi_reportdefault);//更新fact中变量值
                        update(reportcreate_bi_reportbireport);//更新fact中变量值
            end

            //逻辑处理节点[建立bi报表扩展模型]
            rule "deaction1"
            ruleflow-group "reportcreate_bi_reportdeaction1"
            when
            then
                        pssysbireportservice.create(reportcreate_bi_reportbireport);
